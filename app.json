      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package":"com.suresh1030.MyFirstProject"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
